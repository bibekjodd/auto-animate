{
  "name": "@jodd/auto-animate",
  "version": "1.0.4",
  "description": "Zero config animation library for react apps",
  "keywords": [
    "auto-animate",
    "animation",
    "react-animation"
  ],
  "homepage": "https://jodd-auto-animate.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/bibekjodd/auto-animate"
  },
  "license": "MIT",
  "author": {
    "name": "bibekjodd",
    "email": "bibekjodd@gmail.com",
    "url": "https://github.com/bibekjodd"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src && tsc --noEmit",
    "prepublishOnly": "tsup"
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.8.2"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "@eslint/js": "^9.14.0",
    "@types/react": "~18.3.12",
    "@types/react-dom": "~18.3.1",
    "eslint": "^8",
    "eslint-plugin-react": "^7.37.2",
    "globals": "^15.12.0",
    "typescript-eslint": "^8.13.0"
  }
}
